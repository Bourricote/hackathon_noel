<div class="form_mission">{{ form_start(form) }}

    {{ form_row(form.title) }}
    {{ form_row(form.description) }}
    {{ form_row(form.missionType) }}
    {{ form_row(form.level) }}
    {{ form_row(form.departureDate) }}
    {{ form_row(form.returnDate) }}
    {{ form_row(form.transport) }}
    {{ form_row(form.numberPerson) }}
    {{ form_row(form.planet) }}
    <div class="d-flex justify-content-end">
        <button class="btn btn_custom mr-3 mt-3 mb-5 " id="btn_custom">{{ button_label|default('Save') }}</button>
    </div>

    {{ form_end(form) }}
</div>
